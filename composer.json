{
    "name": "osiemsiedem/laravel-imagor",
    "description": "Imagor integration for Laravel.",
    "keywords": ["laravel", "imagor", "vips", "libvips", "image", "processing"],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "OsiemSiedem\\Imagor\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "authors": [
        {
            "name": "Marek Szymczuk",
            "email": "marek@osiemsiedem.com",
            "homepage": "http://osiemsiedem.com"
        }
    ],
    "require-dev": {
        "laravel/pint": "^1.5",
        "pestphp/pest": "^1.22",
        "orchestra/testbench": "^8.0",
        "phpstan/phpstan": "^1.9"
    },
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "OsiemSiedem\\Imagor\\ImagorServiceProvider"
            ],
            "aliases": {
                "Imagor": "OsiemSiedem\\Imagor\\Facades\\Imagor"
            }
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
